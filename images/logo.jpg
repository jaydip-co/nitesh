      function checkIfBoostedShotIsInViewport() {
        var boostedShotContainer = document.querySelector('.js-boosted-shot-container');

        if (!isAdServed && boostedShotContainer && boostedShotContainer.getBoundingClientRect().top - 80 < window.innerHeight) {
          Dribbble.AdNetworks.Boost.load({
            context: "shot",
            placement: 'Shot Sidebar',
            requestId: 'b28080de-ca81-4f03-a631-0df65e530cf9'
          });

          shotContainer.removeEventListener('scroll', scrollListener);
          isAdServed = true;
        }
      }

      // In situations where the overlay or window is too short to scroll, we want to load the ad immeidately
      checkIfBoostedShotIsInViewport();

      function scrollListener() {
        // We only want to serve boosted shots after the boosted shot container is scrolled into the viewport.
        checkIfBoostedShotIsInViewport();
      }

      if (!isAdServed) {
        shotContainer.addEventListener('scroll', scrollListener);
      }
